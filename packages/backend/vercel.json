{
  "version": 2,
  "builds": [
    {
      "src": "packages/dist/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "packages/dist/**",
          "packages/public/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "packagaes/dist/index.js"
    }
  ]
}